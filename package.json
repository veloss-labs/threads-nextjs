{
  "name": "sst-nextjs-template",
  "version": "0.0.1-beta.0",
  "description": "nextjs sst template project",
  "scripts": {
    "cli": "node --experimental-modules ./scripts/run.mjs",
    "cli:dev": "node --experimental-modules ./scripts/run.mjs -e dev",
    "cli:prod": "node --experimental-modules ./scripts/run.mjs -e prod",
    "cli:staging": "node --experimental-modules ./scripts/run.mjs -e staging",
    "dev": "run-s cli dev:*",
    "dev:start": "next",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test 'next dev' http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "deploy:dev": "run-s cli:dev sst:deploy",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev",
    "sst:remove": "sst remove",
    "sst:console": "sst console"
  },
  "keywords": [
    "nextjs",
    "sst",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "dependencies": {
    "@next/font": "^13.1.6",
    "@tanstack/react-query": "^4.24.4",
    "@tanstack/react-query-devtools": "^4.24.4",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "ky": "^0.33.2",
    "ky-universal": "^0.11.0",
    "lodash-es": "^4.17.21",
    "next": "^13.1.6",
    "next-seo": "^5.15.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "sass": "^1.57.1",
    "web-streams-polyfill": "^3.2.1",
    "zod": "^3.20.2",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@anansi/browserslist-config": "^1.4.1",
    "@playwright/test": "^1.30.0",
    "@testing-library/react": "^13.4.0",
    "@type-challenges/utils": "^0.1.1",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "shelljs": "^0.8.5",
    "sst": "^2.0.0-rc.64",
    "start-server-and-test": "^1.15.3",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vitest": "^0.28.3"
  }
}
